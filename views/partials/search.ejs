    <li class="movies_li" id="movies_li">
      <a href="/movies" class="entertain_a" id="movies_a">
        <span class="movies_item" id="movies_item">
          <span class="movies_logo_cont">
            <span class="movies_logo"></span>
          </span>
          <span class="apps_name" id="movies_name">Movies</span>
        </span>
      </a>
    </li>

    <li class="books_li" id="books_li">
      <a href="/books" class="entertain_a" id="books_a">
        <span class="books_item" id="books_item">
          <span class="books_logo_cont">
            <span class="books_logo"></span>
          </span>
          <span class="apps_name" id="books_name">Books</span>
        </span>
      </a>
    </li>
