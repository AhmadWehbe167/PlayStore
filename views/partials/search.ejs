                <span class="dropdown-icon"></span>
              </a>
            </div>
          </button>
          <div class="dropdown-content">
            <% linkArr.forEach(function(link) { %>
            <a href="/<%= linkType %>/<%= link %>"><%= link %></a>
            <% }) %>
          </div>
        </div>
      </div>

      <div>
