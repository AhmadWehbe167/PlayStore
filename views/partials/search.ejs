          <span class="entertain_logo_cont">
            <span class="entertain_logo"></span>
          </span>
          <span class="entertain_name" id="entertain_name">Entertainment</span>
        </span>
      </a>
    </li>


    <li class="apps_li" id="apps_li">
      <a href="/apps" class="entertain_a" id="apps_a">
        <span class="apps_item" id="apps_item">
          <span class="apps_logo_cont">
            <span class="apps_logo"></span>
          </span>
          <span class="apps_name" id="apps_name">Apps</span>
        </span>
      </a>
